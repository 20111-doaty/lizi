    <script>
        // å…¨å±€å˜é‡
        let scene, camera, renderer, controls;
        let particles, particlePositions, particleVelocities, particleAccelerations;
        let handMesh, handPoints = [];
        let currentShape = 'sphere';
        let lastShapeChangeTime = 0;
        let explosionForce = 0;
        let handDetected = false;
        let lastHandPosition = null;
        let handSpeed = 0;
        let palmSize = 0;
        let particlesGeometry, particlesMaterial, particlesSystem;
        let showPhysics = false;
        let physicsMeshes = [];
        let hands = null;
        let videoStream = null;
        
        // ç²’å­ç³»ç»Ÿå¸¸é‡ - å‡å°‘åˆ°10,000ä¸ªç²’å­ä»¥æé«˜æ€§èƒ½
        const PARTICLE_COUNT = 10000;
        const PARTICLE_COLOR = 0x00ffff; // é’è‰²
        const SHAPE_TRANSITION_TIME = 1000; // è¿‡æ¸¡æ—¶é—´ï¼ˆæ¯«ç§’ï¼‰
        
        // åˆå§‹åŒ–Three.jsåœºæ™¯
        function initThreeJS() {
            // åˆ›å»ºåœºæ™¯
            scene = new THREE.Scene();
            scene.background = new THREE.Color(0x000811);
            scene.fog = new THREE.Fog(0x000811, 10, 50);
            
            // åˆ›å»ºç›¸æœº
            camera = new THREE.PerspectiveCamera(60, window.innerWidth / window.innerHeight, 0.1, 1000);
            camera.position.set(0, 0, 25);
            
            // åˆ›å»ºæ¸²æŸ“å™¨
            const canvasContainer = document.getElementById('canvasContainer');
            renderer = new THREE.WebGLRenderer({
                canvas: document.getElementById('threeCanvas'),
                antialias: true,
                alpha: true
            });
            renderer.setSize(canvasContainer.offsetWidth, canvasContainer.offsetHeight);
            renderer.setPixelRatio(Math.min(window.devicePixelRatio, 1.5)); // é™åˆ¶åƒç´ æ¯”ä»¥æé«˜æ€§èƒ½
            
            // æ·»åŠ è½¨é“æ§åˆ¶ï¼ˆè°ƒè¯•ç”¨ï¼‰- ç°åœ¨ä½¿ç”¨ç¨³å®šç‰ˆæœ¬çš„OrbitControls
            if (typeof THREE.OrbitControls !== 'undefined') {
                controls = new THREE.OrbitControls(camera, renderer.domElement);
                controls.enableDamping = true;
                controls.dampingFactor = 0.05;
                controls.enabled = false; // é»˜è®¤ç¦ç”¨ï¼Œä¼˜å…ˆä½¿ç”¨æ‰‹åŠ¿æ§åˆ¶
            } else {
                console.warn('OrbitControlsæœªåŠ è½½ï¼Œå°†ç¦ç”¨è°ƒè¯•æ§åˆ¶');
                controls = null;
            }
            
            // æ·»åŠ å…‰æº
            const ambientLight = new THREE.AmbientLight(0x223344, 0.5);
            scene.add(ambientLight);
            
            const directionalLight = new THREE.DirectionalLight(0x00aaff, 0.8);
            directionalLight.position.set(5, 10, 7);
            scene.add(directionalLight);
            
            const pointLight = new THREE.PointLight(0x0088ff, 0.6, 50);
            pointLight.position.set(-5, 5, 10);
            scene.add(pointLight);
            
            // æ·»åŠ ç½‘æ ¼è¾…åŠ©
            const gridHelper = new THREE.GridHelper(50, 20, 0x004466, 0x002244);
            gridHelper.position.y = -10;
            scene.add(gridHelper);
            
            // åˆå§‹åŒ–ç²’å­ç³»ç»Ÿ
            initParticles();
            
            // åˆå§‹åŒ–æ‰‹éƒ¨æ¨¡å‹
            initHandModel();
            
            // çª—å£å¤§å°è°ƒæ•´äº‹ä»¶
            window.addEventListener('resize', onWindowResize);
        }
        
        // åˆå§‹åŒ–ç²’å­ç³»ç»Ÿ
        function initParticles() {
            // åˆ›å»ºç²’å­å‡ ä½•ä½“
            particlesGeometry = new THREE.BufferGeometry();
            
            // åˆå§‹åŒ–ç²’å­ä½ç½®ã€é€Ÿåº¦å’ŒåŠ é€Ÿåº¦æ•°ç»„
            particlePositions = new Float32Array(PARTICLE_COUNT * 3);
            particleVelocities = new Float32Array(PARTICLE_COUNT * 3);
            particleAccelerations = new Float32Array(PARTICLE_COUNT * 3);
            
            // åˆå§‹ç›®æ ‡ä½ç½®ï¼ˆçƒä½“ï¼‰
            const targetPositions = new Float32Array(PARTICLE_COUNT * 3);
            
            // ä¸ºæ¯ä¸ªç²’å­ç”Ÿæˆéšæœºåˆå§‹ä½ç½®å’Œç›®æ ‡ä½ç½®
            for (let i = 0; i < PARTICLE_COUNT; i++) {
                const i3 = i * 3;
                
                // åˆå§‹ä½ç½® - éšæœºåˆ†å¸ƒåœ¨çƒä½“å‘¨å›´
                const radius = 8 * (0.8 + Math.random() * 0.4);
                const theta = Math.random() * Math.PI * 2;
                const phi = Math.acos(2 * Math.random() - 1);
                
                particlePositions[i3] = radius * Math.sin(phi) * Math.cos(theta);
                particlePositions[i3 + 1] = radius * Math.sin(phi) * Math.sin(theta);
                particlePositions[i3 + 2] = radius * Math.cos(phi);
                
                // ç›®æ ‡ä½ç½® - çƒä½“è¡¨é¢
                targetPositions[i3] = particlePositions[i3];
                targetPositions[i3 + 1] = particlePositions[i3 + 1];
                targetPositions[i3 + 2] = particlePositions[i3 + 2];
                
                // åˆå§‹é€Ÿåº¦ - éšæœºå°é€Ÿåº¦
                particleVelocities[i3] = (Math.random() - 0.5) * 0.02;
                particleVelocities[i3 + 1] = (Math.random() - 0.5) * 0.02;
                particleVelocities[i3 + 2] = (Math.random() - 0.5) * 0.02;
                
                // åˆå§‹åŠ é€Ÿåº¦
                particleAccelerations[i3] = 0;
                particleAccelerations[i3 + 1] = 0;
                particleAccelerations[i3 + 2] = 0;
            }
            
            // è®¾ç½®å‡ ä½•ä½“å±æ€§
            particlesGeometry.setAttribute('position', new THREE.BufferAttribute(particlePositions, 3));
            
            // åˆ›å»ºç²’å­æè´¨
            particlesMaterial = new THREE.PointsMaterial({
                color: PARTICLE_COLOR,
                size: 0.1, // ç¨å¾®å¢å¤§ç²’å­å¤§å°ä»¥è¡¥å¿æ•°é‡å‡å°‘
                transparent: true,
                opacity: 0.8,
                blending: THREE.AdditiveBlending,
                depthWrite: false
            });
            
            // åˆ›å»ºç²’å­ç³»ç»Ÿ
            particlesSystem = new THREE.Points(particlesGeometry, particlesMaterial);
            scene.add(particlesSystem);
            
            // å­˜å‚¨ç›®æ ‡ä½ç½®ä¾›åç»­ä½¿ç”¨
            particlesSystem.userData.targetPositions = targetPositions;
            particlesSystem.userData.originalPositions = particlePositions.slice(); // å¤åˆ¶æ•°ç»„
        }
        
        // åˆå§‹åŒ–æ‰‹éƒ¨æ¨¡å‹
        function initHandModel() {
            // åˆ›å»ºæ‰‹éƒ¨å…³é”®ç‚¹çƒä½“
            const handGeometry = new THREE.SphereGeometry(0.1, 6, 6); // å‡å°‘å‡ ä½•ä½“ç»†èŠ‚ä»¥æé«˜æ€§èƒ½
            const handMaterial = new THREE.MeshBasicMaterial({ color: 0xff4444 });
            
            // åˆ›å»º21ä¸ªå…³é”®ç‚¹ï¼ˆMediaPipe Handsæœ‰21ä¸ªå…³é”®ç‚¹ï¼‰
            for (let i = 0; i < 21; i++) {
                const sphere = new THREE.Mesh(handGeometry, handMaterial);
                sphere.visible = false; // åˆå§‹éšè—
                scene.add(sphere);
                handPoints.push(sphere);
            }
            
            // åˆ›å»ºæ‰‹éƒ¨éª¨æ¶è¿çº¿
            const handConnections = [
                [0, 1], [1, 2], [2, 3], [3, 4], // æ‹‡æŒ‡
                [0, 5], [5, 6], [6, 7], [7, 8], // é£ŸæŒ‡
                [0, 9], [9, 10], [10, 11], [11, 12], // ä¸­æŒ‡
                [0, 13], [13, 14], [14, 15], [15, 16], // æ— åæŒ‡
                [0, 17], [17, 18], [18, 19], [19, 20], // å°æŒ‡
                [5, 9], [9, 13], [13, 17] // æ‰‹æŒè¿æ¥
            ];
            
            // åˆ›å»ºè¿çº¿æè´¨
            const lineMaterial = new THREE.LineBasicMaterial({ color: 0xff8844 });
            
            // åˆ›å»ºè¿çº¿
            handConnections.forEach(connection => {
                const lineGeometry = new THREE.BufferGeometry().setFromPoints([
                    new THREE.Vector3(0, 0, 0),
                    new THREE.Vector3(0, 0, 0)
                ]);
                const line = new THREE.Line(lineGeometry, lineMaterial);
                line.visible = false;
                scene.add(line);
                handPoints.push(line); // å°†è¿çº¿ä¹ŸåŠ å…¥æ‰‹éƒ¨ç‚¹æ•°ç»„ï¼Œä¾¿äºç®¡ç†
            });
        }
        
        // æ›´æ–°æ‰‹éƒ¨æ¨¡å‹
        function updateHandModel(landmarks) {
            if (!landmarks) {
                // éšè—æ‰‹éƒ¨æ¨¡å‹
                handPoints.forEach(point => point.visible = false);
                return;
            }
            
            // æ›´æ–°å…³é”®ç‚¹ä½ç½®
            for (let i = 0; i < 21; i++) {
                const landmark = landmarks[i];
                const point = handPoints[i];
                
                // å°†MediaPipeåæ ‡è½¬æ¢ä¸ºThree.jsåæ ‡
                const x = (landmark.x - 0.5) * 20;
                const y = (0.5 - landmark.y) * 20;
                const z = (landmark.z || 0) * 10;
                
                point.position.set(x, y, z);
                point.visible = true;
            }
            
            // æ›´æ–°è¿çº¿ï¼ˆæ‰‹éƒ¨å…³é”®ç‚¹åçš„å…ƒç´ æ˜¯è¿çº¿ï¼‰
            const connections = [
                [0, 1], [1, 2], [2, 3], [3, 4],
                [0, 5], [5, 6], [6, 7], [7, 8],
                [0, 9], [9, 10], [10, 11], [11, 12],
                [0, 13], [13, 14], [14, 15], [15, 16],
                [0, 17], [17, 18], [18, 19], [19, 20],
                [5, 9], [9, 13], [13, 17]
            ];
            
            for (let i = 0; i < connections.length; i++) {
                const [startIdx, endIdx] = connections[i];
                const line = handPoints[21 + i]; // è¿çº¿ä»ç´¢å¼•21å¼€å§‹
                
                const startPos = handPoints[startIdx].position;
                const endPos = handPoints[endIdx].position;
                
                line.geometry.setFromPoints([startPos, endPos]);
                line.visible = true;
            }
        }
        
        // è®¡ç®—æ‰‹åŠ¿
        function detectGesture(landmarks) {
            if (!landmarks || landmarks.length < 21) return 'none';
            
            // è·å–å…³é”®ç‚¹
            const thumbTip = landmarks[4];
            const indexTip = landmarks[8];
            const middleTip = landmarks[12];
            const ringTip = landmarks[16];
            const pinkyTip = landmarks[20];
            const wrist = landmarks[0];
            
            // è®¡ç®—æŒ‡å°–åˆ°æ‰‹è…•çš„è·ç¦»
            const distance = (p1, p2) => {
                return Math.sqrt(
                    Math.pow(p1.x - p2.x, 2) + 
                    Math.pow(p1.y - p2.y, 2) + 
                    Math.pow(p1.z - p2.z, 2)
                );
            };
            
            // è®¡ç®—æ‰‹æŒä¸­å¿ƒ
            const palmCenter = {
                x: (landmarks[0].x + landmarks[5].x + landmarks[9].x + landmarks[13].x + landmarks[17].x) / 5,
                y: (landmarks[0].y + landmarks[5].y + landmarks[9].y + landmarks[13].y + landmarks[17].y) / 5,
                z: (landmarks[0].z + landmarks[5].z + landmarks[9].z + landmarks[13].z + landmarks[17].z) / 5
            };
            
            // è®¡ç®—æ‰‹æŒå¤§å°ï¼ˆæ‰‹è…•åˆ°ä¸­æŒ‡æ ¹éƒ¨çš„è·ç¦»ï¼‰
            const middleBase = landmarks[9];
            palmSize = distance(wrist, middleBase);
            
            // æŒ‡å°–åˆ°æ‰‹æŒä¸­å¿ƒçš„è·ç¦»
            const thumbDistance = distance(thumbTip, palmCenter);
            const indexDistance = distance(indexTip, palmCenter);
            const middleDistance = distance(middleTip, palmCenter);
            const ringDistance = distance(ringTip, palmCenter);
            const pinkyDistance = distance(pinkyTip, palmCenter);
            
            // å¹³å‡æŒ‡å°–è·ç¦»
            const avgFingerDistance = (indexDistance + middleDistance + ringDistance + pinkyDistance) / 4;
            
            // æ£€æµ‹ä¸åŒæ‰‹åŠ¿
            // 1. å¼ æ‰‹ - æ‰€æœ‰æŒ‡å°–éƒ½è¿œç¦»æ‰‹æŒä¸­å¿ƒ
            if (indexDistance > avgFingerDistance * 0.8 && 
                middleDistance > avgFingerDistance * 0.8 && 
                ringDistance > avgFingerDistance * 0.8 && 
                pinkyDistance > avgFingerDistance * 0.8) {
                return 'open';
            }
            
            // 2. å‰ªåˆ€æ‰‹ - åªæœ‰é£ŸæŒ‡å’Œä¸­æŒ‡ä¼¸å‡º
            if (indexDistance > avgFingerDistance * 0.9 && 
                middleDistance > avgFingerDistance * 0.9 && 
                ringDistance < avgFingerDistance * 0.6 && 
                pinkyDistance < avgFingerDistance * 0.6) {
                return 'scissors';
            }
            
            // 3. æ¡æ‹³ - æ‰€æœ‰æŒ‡å°–éƒ½é è¿‘æ‰‹æŒä¸­å¿ƒ
            if (indexDistance < avgFingerDistance * 0.5 && 
                middleDistance < avgFingerDistance * 0.5 && 
                ringDistance < avgFingerDistance * 0.5 && 
                pinkyDistance < avgFingerDistance * 0.5) {
                return 'fist';
            }
            
            // 4. é£ŸæŒ‡ - åªæœ‰é£ŸæŒ‡ä¼¸å‡º
            if (indexDistance > avgFingerDistance * 0.9 && 
                middleDistance < avgFingerDistance * 0.6 && 
                ringDistance < avgFingerDistance * 0.6 && 
                pinkyDistance < avgFingerDistance * 0.6) {
                return 'point';
            }
            
            // 5. ç«–å¤§æ‹‡æŒ‡ - åªæœ‰æ‹‡æŒ‡ä¼¸å‡º
            if (thumbDistance > avgFingerDistance * 1.2 && 
                indexDistance < avgFingerDistance * 0.6 && 
                middleDistance < avgFingerDistance * 0.6 && 
                ringDistance < avgFingerDistance * 0.6 && 
                pinkyDistance < avgFingerDistance * 0.6) {
                return 'thumb';
            }
            
            return 'none';
        }
        
        // æ›´æ–°ç²’å­ç›®æ ‡å½¢çŠ¶
        function updateParticleShape(shape) {
            if (currentShape === shape) return;
            
            // è®°å½•åˆ‡æ¢æ—¶é—´
            lastShapeChangeTime = Date.now();
            currentShape = shape;
            
            // è§¦å‘çˆ†ç‚¸æ•ˆæœ
            explosionForce = 0.5;
            
            // æ›´æ–°UIæ˜¾ç¤º
            document.getElementById('particleShape').textContent = 
                shape === 'sphere' ? 'çƒä½“' : 
                shape === 'text' ? 'æ–‡å­—' : 
                shape === 'ring' ? 'åœ†ç¯' : 
                shape === 'star' ? 'æ˜Ÿå½¢' : 'çˆ±å¿ƒ';
            
            // æ›´æ–°æ‰‹åŠ¿æ¿€æ´»çŠ¶æ€
            const gestureElements = {
                'sphere': document.getElementById('gesture1'),
                'text': document.getElementById('gesture2'),
                'ring': document.getElementById('gesture3'),
                'star': document.getElementById('gesture4'),
                'heart': document.getElementById('gesture5')
            };
            
            // é‡ç½®æ‰€æœ‰æ¿€æ´»çŠ¶æ€
            Object.values(gestureElements).forEach(el => el.textContent = 'æœªæ¿€æ´»');
            
            // è®¾ç½®å½“å‰æ¿€æ´»çŠ¶æ€
            if (gestureElements[shape]) {
                gestureElements[shape].textContent = 'æ¿€æ´»';
            }
            
            // æ ¹æ®å½¢çŠ¶ç”Ÿæˆæ–°çš„ç›®æ ‡ä½ç½®
            generateTargetPositions(shape);
        }
        
        // ç”Ÿæˆç›®æ ‡ä½ç½®
        function generateTargetPositions(shape) {
            const targetPositions = particlesSystem.userData.targetPositions;
            
            for (let i = 0; i < PARTICLE_COUNT; i++) {
                const i3 = i * 3;
                let x, y, z;
                
                // æ ¹æ®å½¢çŠ¶è®¡ç®—ç›®æ ‡ä½ç½®
                switch(shape) {
                    case 'sphere':
                        // çƒä½“
                        const radius = 8;
                        const theta = Math.random() * Math.PI * 2;
                        const phi = Math.acos(2 * Math.random() - 1);
                        
                        x = radius * Math.sin(phi) * Math.cos(theta);
                        y = radius * Math.sin(phi) * Math.sin(theta);
                        z = radius * Math.cos(phi);
                        break;
                        
                    case 'text':
                        // æ–‡å­— "æˆ‘æ˜¯ Mok"
                        // ä½¿ç”¨æ­£å¼¦æ³¢å’Œéšæœºåˆ†å¸ƒåˆ›å»ºæ–‡å­—æ•ˆæœ
                        const textRow = Math.floor(i / 1250); // è°ƒæ•´ä¸º10,000ä¸ªç²’å­
                        const textCol = i % 1250;
                        
                        if (textRow < 8) {
                            // åˆ›å»º"M"å½¢çŠ¶
                            x = (textCol / 1250 - 0.5) * 20;
                            y = (textRow / 8) * 6 - 3;
                            z = Math.sin(textCol / 1250 * Math.PI * 4) * 2;
                        } else if (textRow < 16) {
                            // åˆ›å»º"ok"å½¢çŠ¶
                            x = (textCol / 1250 - 0.5) * 20 + 8;
                            y = ((textRow - 8) / 8) * 6 - 3;
                            z = Math.cos(textCol / 1250 * Math.PI * 2) * 2;
                        } else {
                            // éšæœºåˆ†å¸ƒ
                            x = (Math.random() - 0.5) * 20;
                            y = (Math.random() - 0.5) * 12;
                            z = (Math.random() - 0.5) * 10;
                        }
                        break;
                        
                    case 'ring':
                        // åœ†ç¯
                        const ringRadius = 10;
                        const ringAngle = (i / PARTICLE_COUNT) * Math.PI * 2;
                        const ringWidth = 2;
                        
                        x = Math.cos(ringAngle) * ringRadius;
                        y = Math.sin(ringAngle) * ringRadius;
                        z = (Math.random() - 0.5) * ringWidth;
                        break;
                        
                    case 'star':
                        // æ˜Ÿå½¢
                        const starAngle = (i / PARTICLE_COUNT) * Math.PI * 10;
                        const starRadius = 8 + Math.sin(starAngle * 5) * 3;
                        
                        x = Math.cos(starAngle) * starRadius;
                        y = Math.sin(starAngle) * starRadius;
                        z = (Math.random() - 0.5) * 4;
                        break;
                        
                    case 'heart':
                        // çˆ±å¿ƒ
                        const heartAngle = (i / PARTICLE_COUNT) * Math.PI * 2;
                        const heartScale = 6;
                        
                        // çˆ±å¿ƒå‚æ•°æ–¹ç¨‹
                        x = heartScale * 16 * Math.pow(Math.sin(heartAngle), 3);
                        y = heartScale * (13 * Math.cos(heartAngle) - 5 * Math.cos(2*heartAngle) - 2 * Math.cos(3*heartAngle) - Math.cos(4*heartAngle));
                        z = (Math.random() - 0.5) * 8;
                        
                        // ç¼©æ”¾å’Œè°ƒæ•´ä½ç½®
                        x = x * 0.04 - 0.5;
                        y = y * 0.04 - 2;
                        break;
                }
                
                targetPositions[i3] = x;
                targetPositions[i3 + 1] = y;
                targetPositions[i3 + 2] = z;
            }
        }
        
        // æ›´æ–°ç‰©ç†ç³»ç»Ÿ
        function updatePhysics() {
            const currentTime = Date.now();
            const timeSinceChange = currentTime - lastShapeChangeTime;
            
            // è®¡ç®—çˆ†ç‚¸åŠ›è¡°å‡
            if (explosionForce > 0) {
                explosionForce -= 0.01;
                if (explosionForce < 0) explosionForce = 0;
            }
            
            // è·å–ç›®æ ‡ä½ç½®
            const targetPositions = particlesSystem.userData.targetPositions;
            
            // æ›´æ–°æ¯ä¸ªç²’å­çš„ç‰©ç†çŠ¶æ€
            for (let i = 0; i < PARTICLE_COUNT; i++) {
                const i3 = i * 3;
                
                // è·å–å½“å‰ä½ç½®å’Œç›®æ ‡ä½ç½®
                const currentX = particlePositions[i3];
                const currentY = particlePositions[i3 + 1];
                const currentZ = particlePositions[i3 + 2];
                
                const targetX = targetPositions[i3];
                const targetY = targetPositions[i3 + 1];
                const targetZ = targetPositions[i3 + 2];
                
                // è®¡ç®—æœå‘ç›®æ ‡çš„åŠ›ï¼ˆå¼¹ç°§åŠ›ï¼‰
                const springForce = 0.02;
                let forceX = (targetX - currentX) * springForce;
                let forceY = (targetY - currentY) * springForce;
                let forceZ = (targetZ - currentZ) * springForce;
                
                // æ·»åŠ çˆ†ç‚¸åŠ›
                if (explosionForce > 0) {
                    // ä»ä¸­å¿ƒå‘å¤–çˆ†ç‚¸
                    const dx = currentX;
                    const dy = currentY;
                    const dz = currentZ;
                    const distance = Math.sqrt(dx*dx + dy*dy + dz*dz);
                    
                    if (distance > 0.1) {
                        const explosionStrength = explosionForce * (1 - Math.min(timeSinceChange / SHAPE_TRANSITION_TIME, 1));
                        forceX += (dx / distance) * explosionStrength;
                        forceY += (dy / distance) * explosionStrength;
                        forceZ += (dz / distance) * explosionStrength;
                    }
                }
                
                // æ·»åŠ æ‰‹éƒ¨äº¤äº’åŠ›
                if (handDetected && handPoints[0].visible) {
                    const handPos = handPoints[0].position;
                    
                    // è®¡ç®—ç²’å­åˆ°æ‰‹éƒ¨çš„è·ç¦»
                    const dx = currentX - handPos.x;
                    const dy = currentY - handPos.y;
                    const dz = currentZ - handPos.z;
                    const distance = Math.sqrt(dx*dx + dy*dy + dz*dz);
                    
                    // å¦‚æœæ‰‹åœ¨ç§»åŠ¨ï¼Œäº§ç”Ÿé£åŠ›
                    if (handSpeed > 0.5 && distance < 10) {
                        const windStrength = Math.min(handSpeed * 0.01, 0.05);
                        forceX += (Math.random() - 0.5) * windStrength;
                        forceY += (Math.random() - 0.5) * windStrength;
                        forceZ += (Math.random() - 0.5) * windStrength;
                    }
                    
                    // æ‰‹æŒå æ¯”å½±å“ç²’å­ç³»ç»Ÿç¼©æ”¾
                    const scaleFactor = 0.5 + palmSize * 10;
                    const targetScale = Math.min(Math.max(scaleFactor, 0.3), 2.0);
                    particlesSystem.scale.lerp(new THREE.Vector3(targetScale, targetScale, targetScale), 0.05);
                }
                
                // åº”ç”¨åŠ›åˆ°åŠ é€Ÿåº¦
                const damping = 0.95; // é˜»å°¼ç³»æ•°
                particleAccelerations[i3] = forceX;
                particleAccelerations[i3 + 1] = forceY;
                particleAccelerations[i3 + 2] = forceZ;
                
                // æ›´æ–°é€Ÿåº¦
                particleVelocities[i3] = particleVelocities[i3] * damping + particleAccelerations[i3];
                particleVelocities[i3 + 1] = particleVelocities[i3 + 1] * damping + particleAccelerations[i3 + 1];
                particleVelocities[i3 + 2] = particleVelocities[i3 + 2] * damping + particleAccelerations[i3 + 2];
                
                // æ›´æ–°ä½ç½®
                particlePositions[i3] += particleVelocities[i3];
                particlePositions[i3 + 1] += particleVelocities[i3 + 1];
                particlePositions[i3 + 2] += particleVelocities[i3 + 2];
            }
            
            // æ›´æ–°å‡ ä½•ä½“ä½ç½®å±æ€§
            particlesGeometry.attributes.position.needsUpdate = true;
            
            // æ›´æ–°ç‰©ç†å¯è§†åŒ–ï¼ˆå¦‚æœå¯ç”¨ï¼‰
            if (showPhysics) {
                updatePhysicsVisualization();
            }
        }
        
        // æ›´æ–°ç‰©ç†å¯è§†åŒ–
        function updatePhysicsVisualization() {
            // æ¸…ç†æ—§çš„ç‰©ç†å¯è§†åŒ–
            physicsMeshes.forEach(mesh => scene.remove(mesh));
            physicsMeshes = [];
            
            // æ˜¾ç¤ºåŠ é€Ÿåº¦å‘é‡
            const vectorMaterial = new THREE.LineBasicMaterial({ color: 0xff0000 });
            
            for (let i = 0; i < PARTICLE_COUNT; i += 100) { // æ¯100ä¸ªç²’å­æ˜¾ç¤ºä¸€ä¸ªå‘é‡
                const i3 = i * 3;
                const startPoint = new THREE.Vector3(
                    particlePositions[i3],
                    particlePositions[i3 + 1],
                    particlePositions[i3 + 2]
                );
                
                const endPoint = new THREE.Vector3(
                    particlePositions[i3] + particleAccelerations[i3] * 50,
                    particlePositions[i3 + 1] + particleAccelerations[i3 + 1] * 50,
                    particlePositions[i3 + 2] + particleAccelerations[i3 + 2] * 50
                );
                
                const lineGeometry = new THREE.BufferGeometry().setFromPoints([startPoint, endPoint]);
                const line = new THREE.Line(lineGeometry, vectorMaterial);
                physicsMeshes.push(line);
                scene.add(line);
            }
        }
        
        // åˆå§‹åŒ–MediaPipe Hands
        async function initMediaPipe() {
            const videoElement = document.getElementById('inputVideo');
            
            // åˆ›å»ºHandså¯¹è±¡
            hands = new Hands({
                locateFile: (file) => {
                    return `https://cdn.jsdelivr.net/npm/@mediapipe/hands/${file}`;
                }
            });
            
            // é…ç½®å‚æ•° - ä½¿ç”¨è¾ƒä½å¤æ‚åº¦ä»¥æé«˜æ€§èƒ½
            hands.setOptions({
                maxNumHands: 1,
                modelComplexity: 0, // é™ä½æ¨¡å‹å¤æ‚åº¦
                minDetectionConfidence: 0.5,
                minTrackingConfidence: 0.5
            });
            
            // è®¾ç½®ç»“æœå›è°ƒ
            hands.onResults((results) => {
                // æ›´æ–°æ‰‹éƒ¨æ£€æµ‹çŠ¶æ€
                handDetected = !!results.multiHandLandmarks && results.multiHandLandmarks.length > 0;
                
                // æ›´æ–°UI
                const handIndicator = document.getElementById('handIndicator');
                const handStatus = document.getElementById('handStatus');
                
                if (handDetected) {
                    handIndicator.className = 'gesture-indicator active';
                    handStatus.textContent = 'å·²æ£€æµ‹';
                    
                    // è·å–æ‰‹éƒ¨å…³é”®ç‚¹
                    const landmarks = results.multiHandLandmarks[0];
                    
                    // æ›´æ–°æ‰‹éƒ¨æ¨¡å‹
                    updateHandModel(landmarks);
                    
                    // è®¡ç®—æ‰‹éƒ¨é€Ÿåº¦
                    if (lastHandPosition) {
                        const currentPosition = landmarks[0]; // ä½¿ç”¨æ‰‹è…•ä½ç½®
                        const dx = currentPosition.x - lastHandPosition.x;
                        const dy = currentPosition.y - lastHandPosition.y;
                        const dz = currentPosition.z - lastHandPosition.z;
                        handSpeed = Math.sqrt(dx*dx + dy*dy + dz*dz) * 100;
                        
                        document.getElementById('handSpeed').textContent = handSpeed.toFixed(2);
                    }
                    
                    lastHandPosition = landmarks[0];
                    
                    // æ£€æµ‹æ‰‹åŠ¿
                    const gesture = detectGesture(landmarks);
                    document.getElementById('currentGesture').textContent = 
                        gesture === 'open' ? 'å¼ æ‰‹' : 
                        gesture === 'scissors' ? 'å‰ªåˆ€æ‰‹' : 
                        gesture === 'fist' ? 'æ¡æ‹³' : 
                        gesture === 'point' ? 'é£ŸæŒ‡' : 
                        gesture === 'thumb' ? 'å¤§æ‹‡æŒ‡' : 'æ— ';
                    
                    // æ ¹æ®æ‰‹åŠ¿æ›´æ–°ç²’å­å½¢çŠ¶
                    switch(gesture) {
                        case 'open': updateParticleShape('sphere'); break;
                        case 'scissors': updateParticleShape('text'); break;
                        case 'fist': updateParticleShape('ring'); break;
                        case 'point': updateParticleShape('star'); break;
                        case 'thumb': updateParticleShape('heart'); break;
                    }
                    
                    // æ›´æ–°æ‰‹æŒå æ¯”æ˜¾ç¤º
                    const palmPercent = Math.min(palmSize * 100, 100);
                    document.getElementById('palmRatio').textContent = palmPercent.toFixed(0) + '%';
                } else {
                    handIndicator.className = 'gesture-indicator';
                    handStatus.textContent = 'æœªæ£€æµ‹';
                    document.getElementById('currentGesture').textContent = 'æ— ';
                    document.getElementById('handSpeed').textContent = '0.00';
                    
                    // éšè—æ‰‹éƒ¨æ¨¡å‹
                    updateHandModel(null);
                }
            });
            
            // åˆå§‹åŒ–æ‘„åƒå¤´
            try {
                // è¯·æ±‚æ‘„åƒå¤´è®¿é—®æƒé™
                videoStream = await navigator.mediaDevices.getUserMedia({
                    video: {
                        width: { ideal: 640 },
                        height: { ideal: 480 },
                        facingMode: 'user'
                    }
                });
                
                // å°†è§†é¢‘æµè¿æ¥åˆ°è§†é¢‘å…ƒç´ 
                videoElement.srcObject = videoStream;
                
                // ç­‰å¾…è§†é¢‘åŠ è½½
                await new Promise((resolve) => {
                    videoElement.onloadedmetadata = () => {
                        resolve();
                    };
                });
                
                // å¼€å§‹å¤„ç†è§†é¢‘å¸§
                processVideoFrame();
                
                // æ›´æ–°åŠ è½½çŠ¶æ€
                setTimeout(() => {
                    document.getElementById('loading').style.display = 'none';
                }, 1000);
                
            } catch (error) {
                console.error('æ— æ³•è®¿é—®æ‘„åƒå¤´:', error);
                document.getElementById('loading').style.display = 'none';
                alert('æ— æ³•è®¿é—®æ‘„åƒå¤´ã€‚è¯·ç¡®ä¿å·²æˆäºˆæ‘„åƒå¤´æƒé™ï¼Œç„¶ååˆ·æ–°é¡µé¢é‡è¯•ã€‚');
            }
        }
        
        // å¤„ç†è§†é¢‘å¸§
        function processVideoFrame() {
            const videoElement = document.getElementById('inputVideo');
            
            // åˆ›å»ºå¤„ç†å¸§çš„å‡½æ•°
            const processFrame = async () => {
                if (videoElement.readyState === videoElement.HAVE_ENOUGH_DATA) {
                    // å°†è§†é¢‘å¸§å‘é€ç»™Handsæ¨¡å‹
                    await hands.send({image: videoElement});
                }
                
                // ç»§ç»­å¤„ç†ä¸‹ä¸€å¸§
                requestAnimationFrame(processFrame);
            };
            
            // å¼€å§‹å¤„ç†å¸§
            processFrame();
        }
        
        // çª—å£å¤§å°è°ƒæ•´å¤„ç†
        function onWindowResize() {
            const canvasContainer = document.getElementById('canvasContainer');
            camera.aspect = canvasContainer.offsetWidth / canvasContainer.offsetHeight;
            camera.updateProjectionMatrix();
            renderer.setSize(canvasContainer.offsetWidth, canvasContainer.offsetHeight);
        }
        
        // åŠ¨ç”»å¾ªç¯
        function animate() {
            requestAnimationFrame(animate);
            
            // æ›´æ–°ç‰©ç†ç³»ç»Ÿ
            updatePhysics();
            
            // æ›´æ–°ç²’å­ç³»ç»Ÿ
            particlesSystem.rotation.y += 0.001;
            
            // æ›´æ–°æ‰‹éƒ¨ç‚¹æ—‹è½¬ï¼ˆä½¿å…¶å§‹ç»ˆé¢å‘ç›¸æœºï¼‰
            if (handDetected && handPoints[0].visible) {
                handPoints.forEach((point, index) => {
                    if (index < 21) { // åªå¯¹å…³é”®ç‚¹çƒä½“è¿›è¡Œæ—‹è½¬
                        point.lookAt(camera.position);
                    }
                });
            }
            
            // æ›´æ–°æ§åˆ¶
            if (controls) {
                controls.update();
            }
            
            // æ¸²æŸ“åœºæ™¯
            renderer.render(scene, camera);
        }
        
        // åˆå§‹åŒ–å‡½æ•°
        function init() {
            // åˆå§‹åŒ–Three.js
            initThreeJS();
            
            // åˆå§‹åŒ–ç²’å­å½¢çŠ¶
            generateTargetPositions('sphere');
            
            // å¼€å§‹åŠ¨ç”»å¾ªç¯
            animate();
            
            // æ·»åŠ äº‹ä»¶ç›‘å¬å™¨
            document.getElementById('resetBtn').addEventListener('click', () => {
                explosionForce = 0.8;
                updateParticleShape('sphere');
            });
            
            document.getElementById('togglePhysics').addEventListener('click', () => {
                showPhysics = !showPhysics;
                
                // æ˜¾ç¤º/éšè—ç‰©ç†å¯è§†åŒ–
                physicsMeshes.forEach(mesh => mesh.visible = showPhysics);
                
                document.getElementById('togglePhysics').textContent = 
                    showPhysics ? 'éšè—ç‰©ç†è§†å›¾' : 'åˆ‡æ¢ç‰©ç†è§†å›¾';
            });
            
            document.getElementById('helpBtn').addEventListener('click', () => {
                alert('WebARç²’å­äº’åŠ¨ç³»ç»Ÿä½¿ç”¨è¯´æ˜ï¼š\n\n1. å…è®¸æ‘„åƒå¤´è®¿é—®æƒé™\n2. å°†æ‰‹æ”¾å…¥æ‘„åƒå¤´è§†é‡ä¸­\n3. ä½¿ç”¨äº”ç§æ‰‹åŠ¿æ§åˆ¶ç²’å­å½¢æ€ï¼š\n   - å¼ å¼€æ‰‹ï¼šç²’å­å˜ä¸ºçƒä½“\n   - å‰ªåˆ€æ‰‹ï¼šç²’å­å˜ä¸º"æˆ‘æ˜¯ Mok"æ–‡å­—\n   - æ¡æ‹³ï¼šç²’å­å˜ä¸ºåœ†ç¯\n   - é£ŸæŒ‡ï¼šç²’å­å˜ä¸ºæ˜Ÿå½¢\n   - ç«–å¤§æ‹‡æŒ‡ï¼šç²’å­å˜ä¸ºçˆ±å¿ƒ\n4. å¿«é€ŸæŒ¥æ‰‹å¯ä»¥å¹æ•£ç²’å­\n5. æ‰‹æŒé è¿‘/è¿œç¦»æ‘„åƒå¤´å¯ä»¥ç¼©æ”¾ç²’å­ç³»ç»Ÿ');
            });
            
